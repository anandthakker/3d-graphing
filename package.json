{
  "name": "threed",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "run-p serve watch-bundle watch-ts",
    "serve": "st --no-cache -d dist -p 1337",
    "watch-bundle": "watchify -v --debug dist/app.js -o dist/main.js",
    "watch-ts": "tsc -w -p tsconfig.json",
    "build": "tsc -p tsconfig.json && browserify --debug dist/app.js -o dist/main.js",
    "bundle-deps": "tsc -p  tsconfig.json && browserify --standalone threed --debug dist/deps-bundle.js > bundle.js",
    "bundle-camera": "browserify --standalone reglCamera node_modules/regl-camera/regl-camera.js > regl-camera-bundle.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "gl-matrix": "^2.8.1",
    "isosurface": "^1.0.0",
    "regl": "^1.3.7",
    "regl-camera": "^2.1.1"
  },
  "devDependencies": {
    "@types/gl-matrix": "^2.4.4",
    "browserify": "^16.2.3",
    "npm-run-all": "^4.1.3",
    "st": "^1.2.2",
    "typescript": "^3.1.3",
    "watchify": "^3.11.0"
  }
}
